%!PS-Adobe-3.0 EPSF-3.0
%%BoundingBox: 0 0 500 500

0.7 setgray

1 setlinejoin
1 setlinecap
250 250 translate
/p0x 10 def
/p0y 10 def
/p1x 30 def
/p1y 70 def
/p2x 80 def
/p2y 20 def
/p3x 90 def
/p3y 90 def

/angle 30 def
/nrotate 360 angle div def
%color setting
/c1r 1 def
/c1g 0.83 def
/c1b 0.83 def

/c2r 0.67 def
/c2g 0 def
/c2b 0.33 def

/dr c2r c1r sub def
/dr dr nrotate div def
/dg c2g c1g sub def
/dg dg nrotate div def
/db c2b c1b sub def
/db db nrotate div def

/r c1r def
/g c1g def
/b c1b def

/setrandomcolor {
  /r rand def
  /r r 100 mod def
  /r r 100 div def
  /g rand def
  /g g 100 mod def
  /g g 100 div def
  /b rand def
  /b b 100 mod def
  /b b 100 div def
  r g b setrgbcolor
} def

r g b setrgbcolor

/n 40 def
1 1 nrotate {
1 1 n {
  /p1y rand def
  /p1y p1y 70 mod def
  /p2y rand def
  /p2y p2y 80 mod def
  newpath %bezier
  p0x p0y moveto
  p1x p1y
  p2x p2y
  p3x p3y curveto
  stroke

} for
  angle rotate
  setrandomcolor
} for

newpath
0 0 15 0 360 arc closepath
fill
setrandomcolor
newpath
0 0 13 0 360 arc closepath
fill
setrandomcolor
newpath
0 0 11 0 360 arc closepath
fill

setrandomcolor
newpath
0 0 9 0 360 arc closepath
fill

showpage

%EOF
